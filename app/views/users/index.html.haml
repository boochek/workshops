.page-header
  %h4= "Admin panel:"
  %h2= "Users"
%table.table.table-striped
  %thead
    %tr
      %th= "Name"
      %th= "E-mail"
      %th= "Admin"
      %th= "Created at"
      %th= "Current IP"
      %th= "Current sign in at"
      %th= "Last IP"
      %th= "Last sign in at"

  %tbody
    - users.each do |user|
      %tr
        %td= user.fullname
        %td= user.email
        %td= user.admin
        %td= d(user.created_at)
        %td= user.current_sign_in_ip
        %td= d(user.current_sign_in_at)
        %td= user.last_sign_in_ip
        %td= d(user.last_sign_in_at)




